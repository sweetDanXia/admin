,\n    name: 'Main',\n    component: Main,\n    children: [{\n      path: '/categories/create',\n      name: 'CategoryEdit',\n      component: CategoryEdit\n    },\n    {\n      path: '/categories/list',\n      name: 'CategoryList',\n      component: CategoryList\n    },\n    {\n      path: '/categories/edit/:id',\n      name: 'CategoryEdit',\n      component: CategoryEdit,\n      // props: true 表示把任何url参数，比如id注入到CategoryEdit页面，在CategoryEdit页面可以直接使用变量id，通过props接收\n      props: true\n    },\n    {\n      path: '/items/create',\n      name: 'ItemEdit',\n      component: ItemEdit\n    },\n    {\n      path: '/items/list',\n      name: 'ItemEdit',\n      component: ItemList\n    },\n    {\n      path: '/items/edit/:id',\n      name: 'ItemEdit',\n      component: ItemEdit,\n      props: true\n    },\n    {\n      path: '/heroes/create',\n      name: 'HeroEdit',\n      component: HeroEdit\n    },\n    {\n      path: '/heroes/list',\n      name: 'HeroEdit',\n      component: HeroList\n    },\n    {\n      path: '/heroes/edit/:id',\n      name: 'HeroEdit',\n      component: HeroEdit,\n      props: true\n    },\n    {\n      path: '/articles/create',\n      name: 'ArticleEdit',\n      component: ArticleEdit\n    },\n    {\n      path: '/articles/list',\n      name: 'ArticleEdit',\n      component: ArticleList\n    },\n    {\n      path: '/articles/edit/:id',\n      name: 'ArticleEdit',\n      component: ArticleEdit,\n      props: true\n    },\n    {\n      path: '/admin_ser/create',\n      name: 'AdminUserEdit',\n      component: AdminUserEdit\n    },\n    {\n      path: '/admin_ser/list',\n      name: 'AdminUserEdit',\n      component: AdminUserList\n    },\n    {\n      path: '/admin_ser/edit/:id',\n      name: 'AdminUserEdit',\n      component: AdminUserEdit,\n      props: true\n    }]\n  }\n]\n\nconst router = new VueRouter({\n  mode: 'history',\n  base: process.env.BASE_URL,\n  routes\n})\n\nexport default router\n"]}]}